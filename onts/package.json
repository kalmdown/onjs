  "homepage": "https://github.com/onshape-public/onshape-ts-drawing-client#readme",
  "dependencies": {
    "death": "^1.1.0",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "log4js": "^6.7.1",
    "minimist": "^1.2.7",
    "mkdirp": "^3.0.1",
    "ngrok": "^4.3.3",
    "randomstring": "^1.2.3",
    "rimraf": "^5.0.5",
    "sanitize-filename": "^1.6.3",
    "time-span": "^5.1.0",
    "typescript": "^4.9.5 ",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/death": "^1.1.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/localtunnel": "^2.0.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.19",
    "@types/randomstring": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0"
  }
